---
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ jenkins_git_config_name }}
  namespace: {{ namespace }}
  labels:
    app: {{ app_name }}
    apb_name: '{{ apb_name }}'
    apb_plan_id: '{{ _apb_plan_id }}'
    apb_service_class_id: '{{ _apb_service_class_id }}'
    apb_service_instance_id: '{{ _apb_service_instance_id }}'
    component: jenkins
data:
  user.name: {{ jenkins_git_release_username }}
  user.email: {{ jenkins_git_release_email }}
